{
    "name": "kynx/expressive-handlebars-v8js",
    "description": "Zend Expressive template adapter for kynx/v8js-handlebars",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Matt Kynaston",
            "email": "matt@kynx.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4" : {
            "Kynx\\Expressive\\Handlebars\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "KynxTest\\Expressive\\Handlebars\\": "test/"
        }
    },
    "require": {
        "php": ">=5.5",
        "kynx/template-resolver": "dev-master",
        "kynx/v8js-handlebars": "dev-develop",
        "kynx/zend-cache-psr6": "dev-master",
        "zendframework/zend-expressive": "~1.0.0-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3",
        "components/handlebars.js": "dev-master"
    }
}
